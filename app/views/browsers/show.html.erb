<% content_for :meta_title, t('.title', name: @entity.name) %>
<% content_for :options do %>
    <ul>
      <li><%= link_to t(:list), admin_browsers_path %></li>
      <li><%= link_to t(:create), new_browser_path %></li>
      <% unless @entity.locked? %>
          <li><%= link_to t(:edit), edit_browser_path(@entity) %></li>
          <li><%= link_to_delete @entity %></li>
      <% end %>
    </ul>
<% end %>

<%= render @entity %>