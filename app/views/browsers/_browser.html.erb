<article class="browser">
  <h1><%= browser.name %></h1>

  <%= flag t('activerecord.attributes.browser.bot'), browser.bot? %>
  <%= flag t('activerecord.attributes.browser.mobile'), browser.mobile? %>

  <section>
    <h1><%= t('agents.index.header') %></h1>
    <%= render partial: 'admin/list_of_entities', locals: { collection: browser.agents } %>
  </section>
</article>
