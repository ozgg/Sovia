<% if current_user_has_role? :administrator %>
    <div class="track">
      <% if item.respond_to?(:agent) && !item.agent.nil? %>
          <div class="agent"><%= link_to item.agent.name, item.agent %></div>
      <% end %>
      <% if item.respond_to?(:ip) && !item.ip.nil? %>
          <div class="ip"><%= item.ip %></div>
      <% end %>
    </div>
<% end %>
