<% content_for :meta_title, @entity.name %>
<% content_for :sidebar do %>
    <div class="options">
      <ul>
        <li><%= link_to t(:list), my_places_path %></li>
        <li><%= link_to t(:create), new_place_path %></li>
        <li><%= link_to t(:edit), edit_place_path(@entity) %></li>
        <li><%= link_to_delete @entity %></li>
      </ul>
    </div>
<% end %>

<%= render @entity %>