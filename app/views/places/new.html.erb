<% content_for :meta_title, t('.title') %>
<% content_for :sidebar do %>
    <div class="options">
      <ul>
        <li><%= link_to t(:list), my_places_path %></li>
      </ul>
    </div>

    <%= render 'sidebar/more_links' %>
<% end %>

<article class="edit-place">
  <h1><%= t('.title') %></h1>

  <%= render partial: 'form', locals: { entity: @entity } %>
</article>
