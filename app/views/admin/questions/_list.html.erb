<% if collection.any? %>
    <ul class="list-of-entities">
      <% collection.each do |question| %>
          <% if question.deleted? %>
              <li class="deleted"><%= t(:deleted_entity) %>, id: <%= question.id %></li>
          <% else %>
              <li><%= render partial: 'admin/questions/entity/in_list', locals: { question: question } %></li>
          <% end %>
      <% end %>
    </ul>
<% else %>
    <%= render 'shared/nothing_found' %>
<% end %>