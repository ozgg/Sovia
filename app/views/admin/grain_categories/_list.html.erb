<% if collection.any? %>
    <ul class="list-of-entities">
      <% collection.each do |category| %>
          <% if category.deleted? %>
              <li><%= t(:deleted_entity) %> id: <%= category.id %></li>
          <% else %>
              <li><%= render partial: 'admin/grain_categories/entity/in_list', locals: { grain_category: category } %></li>
          <% end %>
      <% end %>
    </ul>
<% else %>
    <%= render 'shared/nothing_found' %>
<% end %>
