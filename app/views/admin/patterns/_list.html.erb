<% if collection.any? %>
    <ul class="list-of-entities">
      <% collection.each do |pattern| %>
          <% if pattern.deleted? %>
              <li><%= t(:deleted_entity) %> id: <%= pattern.id %></li>
          <% else %>
              <li><%= render partial: 'admin/patterns/entity/in_list', locals: { pattern: pattern } %></li>
          <% end %>
      <% end %>
    </ul>
<% else %>
    <%= render 'shared/nothing_found' %>
<% end %>
