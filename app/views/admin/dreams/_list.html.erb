<% if collection.any? %>
    <ul class="list-of-entities">
      <% collection.each do |dream| %>
          <% if dream.deleted? %>
              <li class="deleted"><%= t(:deleted_entity) %>, id: <%= dream.id %></li>
          <% else %>
              <li><%= render partial: 'admin/dreams/entity/in_list', locals: { dream: dream } %></li>
          <% end %>
      <% end %>
    </ul>
<% else %>
    <%= render 'shared/nothing_found' %>
<% end %>