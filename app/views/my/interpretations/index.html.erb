<% content_for :meta_title, t('.title', page: current_page) %>
<% content_for :breadcrumbs do %>
  <%= link_to(t('my.index.index.nav_text'), my_path) %>
  <span><%= t('.nav_text') %></span>
<% end %>

<article>
  <div class="content-wrapper">
    <h1><%= t('.heading') %></h1>

    <div class="message-box-info">
      У вас
      <%= t(:interpretation_request_count, count: component_handler.request_count) %>
    </div>

    <%= paginate @collection %>
    <%= render partial: 'list', locals: { collection: @collection } %>
    <%= paginate @collection %>
  </div>
</article>
