<% content_for :meta_title, t('.title', id: @entity.id) %>
<% content_for :options do %>
    <ul>
      <li><%= link_to t(:create), new_token_path %></li>
      <li><%= link_to t(:list), admin_tokens_path %></li>
      <li><%= link_to t(:edit), edit_token_path(@entity) %></li>
      <li><%= link_to_delete @entity %></li>
    </ul>
<% end %>

<%= render @entity %>
