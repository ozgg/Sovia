<% content_for :meta_title, t('.title') + title_for_archive(params[:year], params[:month]) %>

<%= render 'shared/metrics' %>

<article class="dreams">
  <h1><%= t('.title') %></h1>
  <ul>
    <% @dates.keys.each do |year| %>
        <li><%= link_to year, archive_dreams_path(year: year) %></li>
    <% end %>
  </ul>

  <%= render 'month_list' %>
  <%= render 'shared/social' %>
</article>
